{
  "name": "clamin-server",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "node dist/prisma/seed.js"
  },
  "scripts": {
    "dev": "rimraf dist && nest start --watch",
    "build": "nest build",
    "start": "nest start",
    "generate-swagger": "ts-node -r tsconfig-paths/register swagger-generator.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:typecheck": "tsc -p . --noEmit",
    "lint:typecheck:staged": "tsc -p . --noEmit && echo #",
    "lint:eslint": "NODE_OPTIONS='--max_old_space_size=5120' eslint .",
    "lint:prettier": "NODE_OPTIONS='--max_old_space_size=5120' prettier . --check",
    "lint": "yarn lint:typecheck && yarn lint:eslint && yarn lint:prettier",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "fix": "yarn lint:typecheck && yarn fix:eslint && yarn fix:prettier",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "validate-prisma-schema": "prisma validate",
    "generate-prisma-client": "prisma generate --watch",
    "generate-prisma-prod": "prisma generate",
    "seed": "prisma db seed",
    "generate-prisma-dev": "prisma migrate dev --create-only --skip-seed",
    "migrate-prisma-dev": "prisma migrate dev --skip-seed",
    "reset-prisma-dev": "prisma migrate reset",
    "migrate-prisma-prod": "prisma migrate deploy --preview-feature",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.395.0",
    "@aws-sdk/client-s3": "^3.395.0",
    "@aws-sdk/client-ses": "^3.395.0",
    "@aws-sdk/s3-request-presigner": "^3.395.0",
    "@liaoliaots/nestjs-redis-health": "^9.0.4",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@prisma/client": "^5.1.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/jsonwebtoken": "^9.0.2",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "fs": "^0.0.1-security",
    "helmet": "^7.0.0",
    "husky": "^9.0.11",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "luxon": "^3.4.0",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.9.3",
    "nestjs-i18n": "^10.2.6",
    "nestjs-redis": "^1.3.3",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "prisma": "5.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "string-format": "^2.0.0",
    "swagger-ui-express": "^5.0.0",
    "ts-md5": "^1.3.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@tsconfig/node18": "1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/joi": "^17.2.3",
    "@types/luxon": "^3.3.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.5.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "yarn run lint",
      "yarn run format",
      "git add ."
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
